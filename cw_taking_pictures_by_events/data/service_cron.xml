<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="ir_cron_cw_snapshot_image_cleaner" model="ir.cron">
        <field name="name">CW Snapshot cleaner</field>
        <field name="model_id" ref="model_cw_taken_picture_details"/>
        <field name="state">code</field>
        <field name="code">model.clean_non_existing_files()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="active" eval="True"/>
    </record>
</odoo>